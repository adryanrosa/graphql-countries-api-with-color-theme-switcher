{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "next/core-web-vitals",
    "trybe-frontend",
    "plugin:react/jsx-runtime"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": "off"
  }
}